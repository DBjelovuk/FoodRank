{
  "name": "food-rank",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-highcharts": "^16.0.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "serve": "^10.1.2"
  },
  "scripts": {
    "prewatch-css": "node-sass src -o src",
    "watch-css": "node-sass -w src -o src",
    "start": "serve -s build",
    "local": "npm run watch-css && react-scripts start",
    "build": "npm run prewatch-css && react-scripts build",
    "deploy": "npm run build && gcloud app deploy -q -v=1",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass": "^4.11.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
